{
  "name": "@varkes/odata-mock",
  "version": "0.8.0",
  "main": "dist/server/app.js",
  "types": "dist/server/types.d.ts",
  "license": "Apache-2.0",
  "description": "OData Mock",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsc && copyfiles -f -e '**/*.ts' src/test/* dist/test && copyfiles -f src/server/resources/* dist/server/resources",
    "start": "node dist/server/server.js",
    "start:dev": "npm run clean && npm run compile && export DEBUG=express:* && node dist/server/server ../test/varkes_config.json",
    "pretest": "npm run compile",
    "test": "mocha dist/test",
    "clean": "rm -rf dist || true && rm -r logs || true && rm -r generated || true"
  },
  "dependencies": {
    "@varkes/configuration": "^0.8.0",
    "@sap/cds": "^3.21",
    "@sap/cds-dk": "^1.4.2",
    "@sap/edm-converters": "^1.0.19",
    "@sap/cds-odata-v2-adapter-proxy": "^1.4.15",
    "express": "^4.17.1",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "copyfiles": "^2.1.1",
    "mocha": "^7.0.0",
    "supertest": "^4.0.2",
    "typescript": "~3.7.4"
  },
  "repository": {
    "type": "ssh",
    "url": "git@github.com:kyma-incubator/varkes.git"
  }
}
